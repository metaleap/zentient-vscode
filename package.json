{
    "name": "zentient-vscode",
    "version": "1.0.0",
    "publisher": "metaleap",
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/zentient",
    "engines": {
        "vscode": "^1.19.1"
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.6.2",
        "vscode": "^1.1.10",
        "@types/node": "^8.5.2",
        "vscode-debugadapter-testsupport": "^1.25.0"
    },
    "dependencies": {
        "vscode-debugadapter": "^1.25.0",
        "vscode-debugprotocol": "^1.25.0"
    },
    "contributes": {
        "keybindings": [
            {
                "command": "zen.menus.main",
                "key": "ctrl+shift+y"
            },
            {
                "command": "zen.extras.query",
                "key": "ctrl+alt+q"
            },
            {
                "command": "zen.extras.intel",
                "key": "ctrl+alt+i"
            }
        ],
        "_nope_views": {
            "explorer": [
                {
                    "id": "zen.treeView",
                    "name": "⟨ℤ⟩"
                }
            ]
        },
        "commands": [
            {
                "category": "Zentient",
                "command": "zen.menus.main",
                "title": "⟨ℤ⟩ Main Menu",
                "icon": {
                    "dark": "./files/icons/ic_settings_ethernet_white_48dp.png",
                    "light": "./files/icons/ic_settings_ethernet_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.menus.main.alt",
                "title": "⟨ℤ⟩ Main Menu (sticky sub-menus)",
                "icon": {
                    "dark": "./files/icons/ic_settings_ethernet_white_48dp.png",
                    "light": "./files/icons/ic_settings_ethernet_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.internal.objSnap",
                "title": "⟨ℤ·internals⟩ objSnap"
            },
            {
                "category": "Zentient",
                "command": "zen.internal.openFileAt",
                "title": "⟨ℤ·internals⟩ openFileAt"
            },
            {
                "category": "Zentient",
                "command": "zen.folder.openNew",
                "title": "⟨ℤ⟩ Open in New Window"
            },
            {
                "category": "Zentient",
                "command": "zen.folder.openHere",
                "title": "⟨ℤ⟩ Open in This Window"
            },
            {
                "category": "Zentient",
                "command": "zen.folder.favsHere",
                "title": "⟨ℤ⟩ Favorite Folders (Open in This Window)",
                "icon": {
                    "dark": "./files/icons/ic_folder_special_white_48dp.png",
                    "light": "./files/icons/ic_folder_special_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.folder.favsNew",
                "title": "⟨ℤ⟩ Favorite Folders (Open in New Window)",
                "icon": {
                    "dark": "./files/icons/ic_folder_special_white_48dp.png",
                    "light": "./files/icons/ic_folder_special_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.term.favs",
                "title": "⟨ℤ⟩ Terminal Stickies",
                "icon": {
                    "dark": "./files/icons/ic_input_white_48dp.png",
                    "light": "./files/icons/ic_input_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.term.favs.alt",
                "title": "⟨ℤ⟩ Terminal Stickies: Re-run Command Last Picked",
                "icon": {
                    "dark": "./files/icons/ic_input_white_48dp.png",
                    "light": "./files/icons/ic_input_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.extras.intel",
                "title": "⟨ℤ⟩ CodeIntel",
                "icon": {
                    "dark": "./files/icons/ic_lightbulb_outline_white_48dp.png",
                    "light": "./files/icons/ic_lightbulb_outline_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.extras.intel.alt",
                "title": "⟨ℤ⟩ CodeIntel: re-view results of most-recent request",
                "icon": {
                    "dark": "./files/icons/ic_lightbulb_outline_white_48dp.png",
                    "light": "./files/icons/ic_lightbulb_outline_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.extras.query",
                "title": "⟨ℤ⟩ CodeQuery",
                "icon": {
                    "dark": "./files/icons/ic_youtube_searched_for_white_48dp.png",
                    "light": "./files/icons/ic_youtube_searched_for_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.extras.query.alt",
                "title": "⟨ℤ⟩ CodeQuery: re-view results of most-recent request",
                "icon": {
                    "dark": "./files/icons/ic_youtube_searched_for_white_48dp.png",
                    "light": "./files/icons/ic_youtube_searched_for_black_48dp.png"
                }
            }
        ],
        "configuration": {
            "title": "Zentient",
            "properties": {
                "zentient.diagSeverityStickiness": {
                    "type": "integer",
                    "maximum": 3,
                    "minimum": 0,
                    "default": 0,
                    "description": "Auto-Lint diagnostics at-or-below this severity level (0=error, 1=warn, 2=info, 3=hint) are 'sticky', all above are 'non-sticky' (ie. only show while their file is opened and auto-hide when it's closed).\n\nChanging this takes effect for each diagnostics-collection the next time it gets refreshed."
                },
                "zentient.langProgs": {
                    "type": "object",
                    "default": {
                        "go": "zentient-go",
                        "haskell": "zentient-hs",
                        "purescript": "zentient-ps"
                    },
                    "description": "Maps known VS Code language IDs to their corresponding Zentient language provider programs. To disable one, set it empty instead of removing it.\n\nAny changes are picked up after editor reload."
                },
                "zentient.termStickies": {
                    "type": "array",
                    "default": [
                        "git-brutepush"
                    ],
                    "description": "Each specified command may include `${file}`, `${dir}`, `${FILE}`, `${DIR}` or `${arg}` placeholders.",
                    "uniqueItems": true
                },
                "zentient.favFolders": {
                    "type": "array",
                    "default": [
                        "~"
                    ],
                    "description": "Favorite folders for fast opening-as-workspaces. No placeholders, but a beginning of `~` uses the current $HOME / %UserProfile% in-place; and ending in a single `*` lists all direct sub-folders of the given path.",
                    "uniqueItems": true
                }
            }
        },
        "explorer": {},
        "menus": {
            "explorer/context": [
                {
                    "command": "zen.folder.openNew",
                    "group": "navigation"
                },
                {
                    "command": "zen.folder.openHere",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "command": "zen.menus.main",
                    "group": "navigation@1",
                    "alt": "zen.menus.main.alt"
                },
                {
                    "command": "zen.extras.intel",
                    "group": "navigation@2",
                    "alt": "zen.extras.intel.alt"
                },
                {
                    "command": "zen.extras.query",
                    "group": "navigation@3",
                    "alt": "zen.extras.query.alt"
                },
                {
                    "command": "zen.folder.favsHere",
                    "group": "navigation@4",
                    "alt": "zen.folder.favsNew"
                },
                {
                    "command": "zen.term.favs",
                    "group": "navigation@5",
                    "alt": "zen.term.favs.alt"
                }
            ],
            "editor/title/context": []
        },
        "debuggers": [
            {
                "type": "zdbg",
                "program": "bin/zdbg",
                "label": "⟨ℤ⟩",
                "languages": [
                    "go",
                    "haskell"
                ],
                "runtime": "",
                "enableBreakpointsFor": {
                    "languageIds": [
                        "go",
                        "haskell"
                    ]
                },
                "initialConfigurations": [
                    {
                        "type": "zdbg",
                        "name": "⟨ℤ⟩",
                        "request": "launch",
                        "w": "${workspaceRoot}",
                        "c": "${cwd}",
                        "f": "${file}",
                        "s": "${command:zDbgSrc}"
                    }
                ],
                "configurationAttributes": {
                    "launch": {
                        "required": [
                            "w",
                            "c",
                            "f"
                        ],
                        "properties": {
                            "w": {
                                "type": "string",
                                "description": "workspace root dir --- use ${workspaceRoot}",
                                "default": "${workspaceRoot}"
                            },
                            "c": {
                                "type": "string",
                                "description": "current work dir --- use ${cwd}",
                                "default": "${cwd}"
                            },
                            "f": {
                                "type": "string",
                                "description": "full file path to current source file, if any --- use ${file}",
                                "default": "${file}"
                            },
                            "s": {
                                "type": "string",
                                "description": "current file source if modified-unsaved --- use ${command:zDbgSrc}",
                                "default": "${command:zDbgSrc}"
                            }
                        }
                    }
                },
                "variables": {
                    "zDbgSrc": "zen.dbg.curfilesrc"
                }
            }
        ]
    }
}
