{
    "name": "zentient-vscode",
    "version": "1.0.0",
    "publisher": "metaleap",
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/zentient",
    "engines": {
        "vscode": "^1.18.1"
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.8",
        "@types/node": "^8.0.53",
        "vscode-debugadapter-testsupport": "^1.24.0"
    },
    "dependencies": {
        "vscode-debugadapter": "^1.24.0",
        "vscode-debugprotocol": "^1.24.0"
    },
    "contributes": {
        "keybindings": [
            {
                "command": "zen.meta.cmds.listall",
                "key": "F9"
            }
        ],
        "commands": [
            {
                "category": "Zentient",
                "command": "zen.meta.cmds.listall",
                "title": "⟨ℤ⟩ All Commands",
                "icon": {
                    "dark": "./files/icons/ic_settings_ethernet_white_48dp.png",
                    "light": "./files/icons/ic_settings_ethernet_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.vse.dir.openNew",
                "title": "⟨ℤ⟩ Open in New Window"
            },
            {
                "category": "Zentient",
                "command": "zen.vse.dir.openHere",
                "title": "⟨ℤ⟩ Open in This Window"
            },
            {
                "category": "Zentient",
                "command": "zen.folder.favsHere",
                "title": "⟨ℤ⟩ Favorite Folders (Open in This Window)",
                "icon": {
                    "dark": "./files/icons/ic_folder_special_white_48dp.png",
                    "light": "./files/icons/ic_folder_special_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.folder.favsNew",
                "title": "⟨ℤ⟩ Favorite Folders (Open in New Window)",
                "icon": {
                    "dark": "./files/icons/ic_folder_special_white_48dp.png",
                    "light": "./files/icons/ic_folder_special_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.term.favs",
                "title": "⟨ℤ⟩ Terminal Stickies",
                "icon": {
                    "dark": "./files/icons/ic_input_white_48dp.png",
                    "light": "./files/icons/ic_input_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.term.favs.alt",
                "title": "⟨ℤ⟩ Terminal Stickies: Re-run Command Last Picked",
                "icon": {
                    "dark": "./files/icons/ic_input_white_48dp.png",
                    "light": "./files/icons/ic_input_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.tools.intel",
                "title": "⟨ℤ⟩ Code Intel Extras",
                "icon": {
                    "dark": "./files/icons/ic_lightbulb_outline_white_48dp.png",
                    "light": "./files/icons/ic_lightbulb_outline_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.tools.intel.alt",
                "title": "⟨ℤ⟩ Code Intel Extras: Review Last Results",
                "icon": {
                    "dark": "./files/icons/ic_lightbulb_outline_white_48dp.png",
                    "light": "./files/icons/ic_lightbulb_outline_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.tools.query",
                "title": "⟨ℤ⟩ Query Extras",
                "icon": {
                    "dark": "./files/icons/ic_youtube_searched_for_white_48dp.png",
                    "light": "./files/icons/ic_youtube_searched_for_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.tools.query.alt",
                "title": "⟨ℤ⟩ Query Extras: Reuse Querier Last Picked",
                "icon": {
                    "dark": "./files/icons/ic_youtube_searched_for_white_48dp.png",
                    "light": "./files/icons/ic_youtube_searched_for_black_48dp.png"
                }
            }
        ],
        "configuration": {
            "title": "Zentient",
            "properties": {
                "zentient.langProgs": {
                    "type": "object",
                    "default": {
                        "go": "zentient-go",
                        "haskell": "zentient-hs",
                        "purescript": "zentient-ps"
                    },
                    "description": "Maps known VS Code language IDs to their corresponding Zentient language provider programs"
                },
                "zentient.termStickies": {
                    "type": "array",
                    "default": [
                        "git-brutepush"
                    ],
                    "description": "Each specified command may include `${file}`, `${dir}`, `${FILE}`, `${DIR}` or `${arg}` placeholders.",
                    "uniqueItems": true
                },
                "zentient.favFolders": {
                    "type": "array",
                    "default": [
                        "~"
                    ],
                    "description": "Favorite folders for fast opening-as-workspaces. No placeholders, but a beginning of `~` uses the current $HOME / %UserProfile% in-place; and ending in a single `*` lists all direct sub-folders of the given path.",
                    "uniqueItems": true
                }
            }
        },
        "explorer": {},
        "menus": {
            "explorer/context": [
                {
                    "command": "zen.vse.dir.openNew",
                    "group": "navigation"
                },
                {
                    "command": "zen.vse.dir.openHere",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "command": "zen.meta.cmds.listall",
                    "group": "navigation@1"
                },
                {
                    "command": "zen.tools.intel",
                    "group": "navigation@2",
                    "alt": "zen.tools.intel.alt"
                },
                {
                    "command": "zen.tools.query",
                    "group": "navigation@3",
                    "alt": "zen.tools.query.alt"
                },
                {
                    "command": "zen.folder.favsHere",
                    "group": "navigation@4",
                    "alt": "zen.folder.favsNew"
                },
                {
                    "command": "zen.term.favs",
                    "group": "navigation@5",
                    "alt": "zen.term.favs.alt"
                }
            ],
            "editor/title/context": []
        },
        "debuggers": [
            {
                "type": "zdbg",
                "program": "bin/zdbg",
                "label": "⟨ℤ⟩",
                "languages": [
                    "go",
                    "haskell"
                ],
                "runtime": "",
                "enableBreakpointsFor": {
                    "languageIds": [
                        "go",
                        "haskell"
                    ]
                },
                "initialConfigurations": [
                    {
                        "type": "zdbg",
                        "name": "⟨ℤ⟩",
                        "request": "launch",
                        "w": "${workspaceRoot}",
                        "c": "${cwd}",
                        "f": "${file}",
                        "s": "${command:zDbgSrc}"
                    }
                ],
                "configurationAttributes": {
                    "launch": {
                        "required": [
                            "w",
                            "c",
                            "f"
                        ],
                        "properties": {
                            "w": {
                                "type": "string",
                                "description": "workspace root dir --- use ${workspaceRoot}",
                                "default": "${workspaceRoot}"
                            },
                            "c": {
                                "type": "string",
                                "description": "current work dir --- use ${cwd}",
                                "default": "${cwd}"
                            },
                            "f": {
                                "type": "string",
                                "description": "full file path to current source file, if any --- use ${file}",
                                "default": "${file}"
                            },
                            "s": {
                                "type": "string",
                                "description": "current file source if modified-unsaved --- use ${command:zDbgSrc}",
                                "default": "${command:zDbgSrc}"
                            }
                        }
                    }
                },
                "variables": {
                    "zDbgSrc": "zen.dbg.curfilesrc"
                }
            }
        ]
    }
}
