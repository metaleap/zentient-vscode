{
    "name": "zentient-vscode",
    "version": "1.0.0",
    "publisher": "metaleap",
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/zentient",
    "engines": {
        "vscode": "^1.36.1"
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^3.5.3",
        "vscode": "^1.1.35",
        "@types/node": "^12.6.8",
        "vscode-debugadapter-testsupport": "^1.35.0"
    },
    "dependencies": {
        "vscode-debugadapter": "^1.35.0",
        "vscode-debugprotocol": "^1.35.0"
    },
    "contributes": {
        "keybindings": [
            {
                "command": "zen.menus.main",
                "key": "ctrl+shift+y"
            },
            {
                "command": "zen.extras.query",
                "key": "ctrl+alt+q"
            },
            {
                "command": "zen.extras.intel",
                "key": "ctrl+alt+i"
            },
            {
                "command": "zen.term.favs",
                "key": "ctrl+alt+t"
            }
        ],
        "_nope_commands": [
            {
                "command": "zen.internal.objSnap",
                "title": "zen.internal.objSnap"
            }
        ],
        "commands": [
            {
                "category": "Zentient",
                "command": "zen.menus.main",
                "title": "⟨ℤ⟩ Main Menu",
                "icon": {
                    "dark": "./files/icons/ic_settings_ethernet_white_48dp.png",
                    "light": "./files/icons/ic_settings_ethernet_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.menus.main.alt",
                "title": "⟨ℤ⟩ Main Menu (sticky sub-menus)",
                "icon": {
                    "dark": "./files/icons/ic_settings_ethernet_white_48dp.png",
                    "light": "./files/icons/ic_settings_ethernet_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.folder.favsHere",
                "title": "⟨ℤ⟩ Favorite Folders (Open in This Window)",
                "icon": {
                    "dark": "./files/icons/ic_folder_special_white_48dp.png",
                    "light": "./files/icons/ic_folder_special_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.folder.favsNew",
                "title": "⟨ℤ⟩ Favorite Folders (Open in New Window)",
                "icon": {
                    "dark": "./files/icons/ic_folder_special_white_48dp.png",
                    "light": "./files/icons/ic_folder_special_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.term.favs",
                "title": "⟨ℤ⟩ Terminal Stickies",
                "icon": {
                    "dark": "./files/icons/ic_input_white_48dp.png",
                    "light": "./files/icons/ic_input_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.term.favs.alt",
                "title": "⟨ℤ⟩ Terminal Stickies: Re-run Command Last Picked",
                "icon": {
                    "dark": "./files/icons/ic_input_white_48dp.png",
                    "light": "./files/icons/ic_input_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.extras.intel",
                "title": "⟨ℤ⟩ CodeIntel",
                "icon": {
                    "dark": "./files/icons/ic_lightbulb_outline_white_48dp.png",
                    "light": "./files/icons/ic_lightbulb_outline_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.extras.intel.alt",
                "title": "⟨ℤ⟩ CodeIntel: re-view results of most-recent request",
                "icon": {
                    "dark": "./files/icons/ic_lightbulb_outline_white_48dp.png",
                    "light": "./files/icons/ic_lightbulb_outline_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.extras.query",
                "title": "⟨ℤ⟩ CodeQuery",
                "icon": {
                    "dark": "./files/icons/ic_youtube_searched_for_white_48dp.png",
                    "light": "./files/icons/ic_youtube_searched_for_black_48dp.png"
                }
            },
            {
                "category": "Zentient",
                "command": "zen.extras.query.alt",
                "title": "⟨ℤ⟩ CodeQuery: re-view results of most-recent request",
                "icon": {
                    "dark": "./files/icons/ic_youtube_searched_for_white_48dp.png",
                    "light": "./files/icons/ic_youtube_searched_for_black_48dp.png"
                }
            }
        ],
        "languages": [
            {
                "id": "atmo",
                "aliases": [
                    "atmo",
                    "Atmo"
                ],
                "extensions": [
                    ".at"
                ],
                "configuration": "./atmo.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "atmo",
                "scopeName": "source.atmo",
                "path": "./atmo.svo.tmGrammar.json"
            }
        ],
        "configuration": {
            "title": "Zentient",
            "properties": {
                "zentient.langProgs": {
                    "type": "object",
                    "default": {
                        "atmo": "zlang-atmo",
                        "go": "zlang-go",
                        "haskell": "zlang-haskell"
                    },
                    "description": "Maps known VS Code language IDs to their corresponding Zentient language provider programs.\n\nAny changes are picked up after editor reload."
                },
                "zentient.liveLangs": {
                    "type": "array",
                    "default": [
                        "atmo"
                    ]
                },
                "zentient.termStickies": {
                    "type": "array",
                    "default": [
                        "git-brutepush"
                    ],
                    "description": "Each specified command may include `${file}`, `${dir}`, `${FILE}`, `${DIR}` or `${arg}` placeholders.",
                    "uniqueItems": true
                },
                "zentient.favFolders": {
                    "type": "array",
                    "default": [
                        "~"
                    ],
                    "description": "Favorite folders for fast opening-as-workspaces. No placeholders, but a beginning of `~` uses the current $HOME / %UserProfile% in-place; and ending in a single `*` lists all direct sub-folders of the given path.",
                    "uniqueItems": true
                },
                "zentient.darkThemedPages": {
                    "type": "boolean",
                    "default": true
                },
                "zentient.highlightMultipleOnly": {
                    "type": "boolean",
                    "default": false
                }
            }
        },
        "menus": {
            "editor/title": [
                {
                    "command": "zen.menus.main",
                    "group": "navigation@1",
                    "alt": "zen.menus.main.alt"
                },
                {
                    "command": "zen.extras.intel",
                    "group": "navigation@2",
                    "alt": "zen.extras.intel.alt"
                },
                {
                    "command": "zen.extras.query",
                    "group": "navigation@3",
                    "alt": "zen.extras.query.alt"
                },
                {
                    "command": "zen.folder.favsHere",
                    "group": "navigation@4",
                    "alt": "zen.folder.favsNew"
                },
                {
                    "command": "zen.term.favs",
                    "group": "navigation@5",
                    "alt": "zen.term.favs.alt"
                }
            ],
            "editor/title/context": []
        },
        "debuggers": [
            {
                "type": "zentient.dbg",
                "label": "⟨ℤ⟩",
                "languages": [
                    "go"
                ],
                "adapterExecutableCommand": "zentient.dbg.progInfo"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "zentient",
                    "icon": "./files/icons/ic_extension_white_24dp.png",
                    "title": "⟨ℤ⟩ Zentient"
                }
            ]
        }
    }
}
