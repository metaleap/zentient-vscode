{
    "comments": {
        "lineComment": "//",
        "blockComment": [
            "/*",
            "*/"
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string"
            ]
        },
        {
            "open": "'",
            "close": "'",
            "notIn": [
                "string"
            ]
        },
        {
            "open": "`",
            "close": "`",
            "notIn": [
                "string"
            ]
        }
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "'",
            "'"
        ],
        [
            "\"",
            "\""
        ],
        [
            "`",
            "`"
        ]
    ],
    "folding": {
        "offSide": true
    },
    "indentationRules": {
        "increaseIndentPattern": "^.*(\\bcase\\b.*:|\\bdefault\\b:|(\\b(func|if|else|switch|select|for|struct)\\b.*)?{[^}]*|\\([^)]*)$",
        "decreaseIndentPattern": "^\\s*(\\bcase\\b.*:|\\bdefault\\b:|}[)}]*[),]?|\\)[,]?)$"
    }
}
